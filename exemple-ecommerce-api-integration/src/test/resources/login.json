{
	"$schema": "http://json-schema.org/draft-07/schema",
	"additionalProperties": false,
	"required": [
		"password"
	],
	"properties": {
		"login": {
			"type": "string",
			"readOnly": true
		},
		"id": {
			"type": "number",
			"readOnly": true
		},
		"password": {
			"type": "string",
			"writeOnly": true
		},
		"enable": {
			"type": "boolean"
		}
	}
}